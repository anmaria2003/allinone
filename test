Unit tests 
package arithmetics; 
public class Arith_operation { 
public int add(int num1, int num2) { 
return num1+num2; 
} 
public int sub(int num1, int num2) { 
return num1-num2; 
} 
public int Square(int num) { 
return num*num; 
} 
b. multiplication 
public int Multiplication(int num1, int num2) { 
return num1*num2; 
} 
} 

